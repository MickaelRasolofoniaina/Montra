{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "noEmit": true,
    "target": "es6",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "models/*": [
        "models/*"
      ],
      "types/*": [
        "types/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "images/*": [
        "assets/images/*"
      ],
      "constants/*": [
        "constants/*"
      ],
      "components/*": [
        "components/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "layouts/*": [
        "layouts/*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
